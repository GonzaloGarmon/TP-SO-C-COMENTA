SET AX 10
SET BX 80
IO_FS_CREATE FS salida.txt
IO_GEN_SLEEP GENERICA 23
IO_FS_CREATE FS cronologico.txt
IO_FS_TRUNCATE FS salida.txt BX
IO_FS_TRUNCATE FS cronologico.txt BX
SET CX 0
SET DX 9
SET EAX 69
MOV_OUT DX AX
SET DX 19
MOV_OUT DX AX
SET DX 29
MOV_OUT DX AX
SET DX 48
IO_GEN_SLEEP GENERICA 27
MOV_OUT DX AX
SET DX 58
MOV_OUT DX AX
IO_FS_DELETE FS salida.txt BX
SET SI 59
SET DI 80
SET BX 90
MOV_OUT BX AX
SET SI 0
SET DI 91
SET ECX 80
SET AX 10
IO_FS_CREATE FS archivo1.txt
IO_FS_CREATE FS archivo2.txt
IO_FS_TRUNCATE FS archivo1.txt AX
IO_FS_TRUNCATE FS archivo2.txt AX
SET AX 0
SET BX 69
SET CX 80
SET AX 0
SET BX 250
SET CX 70
IO_FS_CREATE FS pesado.txt
IO_FS_TRUNCATE FS pesado.txt BX
IO_FS_DELETE FS archivo1.txt
IO_FS_TRUNCATE FS archivo2.txt CX
SET DX 0
EXIT